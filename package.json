{
  "name": "residence-crawler",
  "version": "0.0.1",
  "main": "handler.js",
  "scripts": {
    "format": "prettier --config .prettierrc --write \"src/**/*.{ts,js}\"",
    "docs": "typedoc --tsconfig tsconfig.json --entryPoints src/**/*.ts --out docs/",
    "cron:watch": "pm2 start src/cron.js --watch",
    "cron:start": "pm2 start src/cron.js",
    "cron:stop": "pm2 stop src/cron.js",
    "pm2": "pm2",
    "tsx": "tsx",
    "crawler": "tsx src/cli.ts",
    "sequelize": "sequelize",
    "sequelize:db:create": "sequelize db:create --charset utf8mb4 --collate utf8mb4_general_ci"
  },
  "author": "TakuKobayashi",
  "private": true,
  "dependencies": {
    "@geolonia/normalize-japanese-addresses": "^3.1.3",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "geohashing": "^2.0.1",
    "lodash": "^4.17.21",
    "mysql2": "^3.9.7",
    "node-html-parser": "^7.0.1",
    "sequelize": "^6.37.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "commander": "^12.0.0",
    "cron": "^3.5.0",
    "documentation": "^14.0.3",
    "dotenv": "^16.4.5",
    "fast-glob": "^3.3.2",
    "pm2": "^5.4.3",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "tsx": "^4.19.2",
    "typedoc": "^0.27.5",
    "typescript": "^5.4.5",
    "typings": "^2.1.1"
  },
  "license": "MIT"
}
